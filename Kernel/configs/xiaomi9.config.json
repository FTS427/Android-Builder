[
  {
    "kernelSource": {
      "name": "lineage",
      "repo": "https://github.com/FTS427/android_kernel_lenovo_x306x-10",
      "branch": "lineage-17.1",
      "device": "amar_row_lte",
      "defconfig": "amar_row_lte_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master-kernel-build-2022/clang-r450784d.tar.gz",
        "binaryEnv": ["./bin"]
      },
      {
        "name": "gcc",
        "repo": "https://githubfast.com/LineageOS/android_prebuilts_gcc_linux-x86_aarch64_aarch64-linux-android-4.9",
        "branch": "lineage-17.1",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "gcc/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
