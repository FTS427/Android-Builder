[
  {
    "kernelSource": {
      "name": "lineage",
      "repo": "https://github.com/FTS427/android_kernel_lenovo_x306x-10",
      "branch": "lineage-17.1",
      "device": "amar_row_lte",
      "defconfig": "amar_row_lte_defconfig"
    },
    "toolchains": [
      {
        "name": "proton-clang",
        "repo": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/master-kernel-build-2022/clang-r450784d.tar.gz",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "proton-clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "proton-clang/bin/arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": false,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
