[
  {
    "kernelSource": {
      "name": "lineage",
      "repo": "https://github.com/FTS427/android_kernel_lenovo_x306x-10",
      "branch": "lineage-17.1",
      "device": "amar_row_lte",
      "defconfig": "amar_row_lte_defconfig"
    },
    "toolchains": [
      {
        "name": "clang",
        "repo": "https://github.com/Cyborg2017/prebuilts_clang_host_linux-x86_clang-r353983c",
        "branch": "master",
        "binaryEnv": ["./bin"]
      },
      {
        "name": "gcc",
        "repo": "https://github.com/FTS427/platform_prebuilts_gcc",
        "branch": "main",
        "binaryEnv": ["./bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "gcc/bin/aarch64-linux-android-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com//FTS427/AnyKernel3",
        "branch": "master"
      }
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
